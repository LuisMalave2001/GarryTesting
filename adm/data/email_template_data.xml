<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="email_template_data_inquiry_accepted" model="mail.template">
        <field name="name">Inquiry Accepted</field>
        <field name="model_id" ref="adm.model_adm_inquiry" />
        <field name="subject">You are now in applicant process</field>
        <field name="email_to"></field>
        <field name="partner_to"></field>
        <field name="body_html" type="html">
            <table border="0" cellpadding="0" cellspacing="0" width="590"
                style="min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;">
                <tr>
                    <td valign="top">
                        Congratulation, but you are already in application process!
                    </td>
                </tr>
            </table>
        </field>
        <field name="user_signature" eval="False" />
        <field name="auto_delete" eval="True" />
        <field name="lang"></field>
    </record>
    
